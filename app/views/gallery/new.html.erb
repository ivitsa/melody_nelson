<h1>Gallery#new</h1>
<p>Find me in app/views/gallery/new.html.erb</p>
<%= form_for @photo, :html => {:multipart => 'true'} do |p| %>
	<p>
	<%= p.text_field :description %><br/>
	<%= p.text_field :position %><br/>
	<%= p.file_field :image %><br/>
	<%= p.submit "Upload" %>
	</p>
<% end %>
<%=link_to "Back to Edit Images", action: "editimages" %>
