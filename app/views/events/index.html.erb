<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>
<div id="notice"><%= flash[:notice] %></div>
<% @events.each do |e| %>
	<p><%=e.headline %><br/>
	<%= e.time %><br/>
	<%= image_tag e.image.url(:medium) %><br/>
	<%= e.description %><br/>
	<%= link_to "Update", edit_event_path(e) %>
	<%= link_to "Delete", event_path(e), method: "delete", data: { confirm: 'Are you sure?'} %>
	</p>
<% end %>
	<p>
	<%= link_to "New event", action: "new" %>
	</p>
