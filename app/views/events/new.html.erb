<h1>Events#new</h1>
<p>Find me in app/views/events/new.html.erb</p>
<%= form_for @event do |e| %>
	<p>
	<%= e.text_field :headline %><br/>
	<%= e.text_area :description, size: "60x12" %><br/>
	<%= e.datetime_select :time %><br/>
	<%= e.submit "New Event" %>
	</p>
<% end %>
<%=link_to "Back", events_path %>
